<%= render 'shared/form/error_message' , record: resume %>

<%= form_for(resume) do |f| %>
<div class="field">
  <%= f.label :name, '姓名' %>
  <%= f.text_field :name %>
</div>

<div class="field">
  <%= f.label :email, '信箱' %>
  <%= f.email_field :email %>
</div>

<div class="field">
  <%= f.label :tel, '電話' %>
  <%= f.telephone_field :tel %>
</div>

<div class="field">
  <%= f.label :skill, '技能' %>
  <%= f.text_field :skill %>
</div>

<div class="field">
  <%= f.label :intro, '簡介' %>
  <%= f.text_area :intro %>
</div>

<div class="field">
  <%= f.label :city, '城市' %>
  <%= f.text_field :city %>
</div>

<div class="field">
  <%= f.label :education, '教育程度' %>
  <%= f.text_area :education %>
</div>

<div class="field">
  <%= f.label :experience, '經驗' %>
  <%= f.text_area :experience %>
</div>

<div class="field">
  <%= f.label :portfolio, '職務' %>
  <%= f.text_area :portfolio %>
</div>



<hr>
<%= f.submit %>
<% end %>



